<%- include("partials/headers"); %>
<h1>
	SIGNUP 
</h1>
<div>
	<form action="/register" method="POST">
		<div class="form-group">
			<input class="form-control" type="text" name="username" placeholder="UserName">
		</div>
		
		<div class="form-group">
			<input type="form-control" type="password" name="password" placeholder="Password">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="email" placeholder="abc@gmail.com">
		</div>
		<div class="form-group">
			<input type="form-control" type="text" name="number" placeholder="Contact Number">
		</div>
		<div  class="form-group">
			<button class="btn btn-primary btn-block">
				SIGNUP
			</button>
		</div>
	</form>
</div>